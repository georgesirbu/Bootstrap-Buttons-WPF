<Application x:Class="TestBackup.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestBackup"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="FontAwesome">/Fonts/FAFree-Regular-400.otf#Font Awesome 5 Free Regular</FontFamily>
            <FontFamily x:Key="FontAwesomeSolid">/Fonts/FAFree-Solid-900.otf#Font Awesome 5 Free Solid</FontFamily>
            <FontFamily x:Key="FontOpenSans">/Fonts/OpenSans-Regular.ttf#Open Sans</FontFamily>
            <FontFamily x:Key="FontOpenSansBold">/Fonts/FAFree-Solid-900.otf#Open Sans</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Stile.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
